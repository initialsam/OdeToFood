@page
@model ListModel
@{
}
<h1>Restaurants</h1>
<div>@Model.Message</div>
<div>@Model.AppSettingMessage</div>
<div>
    <table class="table">
        @foreach (var item in Model.Restaurants)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Location</td>
                <td>@item.Cuisine</td>
            </tr>
        }
    </table>

</div>



